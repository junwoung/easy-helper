{
  "name": "easyh",
  "version": "0.1.0",
  "description": "a project for helping developer to handle some small things",
  "main": "index.js",
  "files": [
    "es",
    "lib",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf es lib",
    "dev:es": "rm -rf es && tsc -w",
    "dev:lib": "rm -rf lib && tsc -p ./tsconfig.lib.json",
    "build": "yarn clean && tsc && tsc -p ./tsconfig.lib.json",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "release": "yarn lint && yarn build && node ./scripts/release.js"
  },
  "repository": "https://github.com/junwoung/easy-helper",
  "author": "junwoung",
  "license": "MIT",
  "devDependencies": {
    "@types/inquirer": "^8.1.1",
    "@types/node": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "chalk": "^4.1.2",
    "eslint": "^7.32.0",
    "inquirer": "^8.1.5",
    "react": "^17.0.2",
    "tslint": "^6.1.3"
  },
  "bin": {
    "easyh": "./bin/index.js"
  },
  "dependencies": {
    "commander": "^8.2.0",
    "download-git-repo": "^3.0.2"
  }
}
